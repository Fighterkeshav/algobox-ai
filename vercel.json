{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/go/(.*)",
            "destination": "/api/go/$1"
        },
        {
            "source": "/api/inngest",
            "destination": "/api/inngest"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/go/*.go": {
            "runtime": "go1.x"
        }
    }
}